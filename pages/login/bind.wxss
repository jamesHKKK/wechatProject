.page-section{
  display: block;
  text-align: center;
  padding:100rpx 150rpx 0;
}
.page-section-image{
  width:156rpx;
  height: 156rpx;
  margin: 0 auto;
  
}
.page-section-image image{
  width: 100%;
  height: 100%;
}
.page-section-view-item{
  width:100%;
  margin: 80rpx 0 40rpx;
}
.page-section-view-item button{
  width: 100% !important;
  height:90rpx !important;
  
  background-color: rgba(211, 216, 222, 1);
  color:#fff;
  border-radius:90rpx;
  font-size: 36rpx;
  font-weight: 500;
  line-height: 55rpx !important;
}
.page-item{
  margin: 80rpx 0;
}
.page-item button{
  background-color: rgba(0, 120, 248, 1);
}