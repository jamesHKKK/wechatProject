
 .timeBox{
  width: 100%;
  height: 100rpx;
  background: #fff;
}
.textBox{
  padding: 20rpx;
}
.statisticsTime{
  font-size: 12px;
  margin-top: 10rpx;
}

/* 表格 */
.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #fff;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
} 